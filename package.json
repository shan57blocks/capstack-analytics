{
  "name": "analytics",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "4.4.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "cra-template": "1.0.3",
    "customize-cra": "^0.9.1",
    "dotenv": "^8.2.0",
    "flux-standard-action": "^2.1.1",
    "font-awesome": "4.7.0",
    "immutability-helper": "^3.1.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add ."
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:prod": "env-cmd -f .env react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "build:test": "env-cmd -f .env.test react-app-rewired build",
    "build:prod": "env-cmd -f .env.production react-app-rewired build",
    "deploy:test": "yarn build:test && cross-env NODE_ENV=test node ./deploy.js",
    "deploy:prod": "yarn build:prod && cross-env NODE_ENV=production node ./deploy.js",
    "deploy": "yarn deploy:test && yarn deploy:prod"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "ftp-deploy": "^2.3.7",
    "gh-pages": "^2.2.0",
    "husky": "^4.3.0",
    "node-banner": "^1.4.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2
  }
}
